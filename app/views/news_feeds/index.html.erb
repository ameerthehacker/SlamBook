<h1 class="page-header text-center">Your News Feeds</h1>
<div class="row">
    <div class="col-sm-2">
        <%= link_to new_book_path, :class => 'btn btn-success' do %>
            <i class="fa fa-plus"></i> New SlamBook
        <% end %>
    </div>
</div>
<div class="row"> 
    <div class="col-sm-12">
        <h4>People you may know</h4>
    </div>
    <div class="users">
        <% current_user.suggested_users.each do |user| %>
            <% if Following.where(:user_id => user.id, :follower_id => current_user.id).count == 0 && user!=current_user %>        
                <div class="col-md-4 col-sm-6 col-xs-12">
                    <%= render user %>
                </div>
            <% end %>
        <% end %>
    </div>
</div>
<%= render @news_feeds %>