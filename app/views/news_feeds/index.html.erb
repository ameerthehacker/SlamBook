<h1 class="page-header text-center">Your News Feeds</h1>
<div class="row">
    <div class="col-sm-2">
        <%= link_to new_book_path, :class => 'btn btn-success' do %>
            <i class="fa fa-plus"></i> New SlamBook
        <% end %>
    </div>
</div>
<div class="suggested-users">
    <%= render 'shared/suggested_users' %>
</div>
<div class="row">
    <div class="col-sm-12"> 
        <%= will_paginate @news_feeds, renderer: BootstrapPagination::Rails %>
    </div>
</div>
<div class="news-feeds">
    <%= render @news_feeds %>
</div>
<div class="row">
    <div class="col-sm-12"> 
        <%= will_paginate @news_feeds, renderer: BootstrapPagination::Rails %>
    </div>
</div>